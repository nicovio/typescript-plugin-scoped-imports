{
  "name": "typescript-plugin-scoped-imports-workspace",
  "version": "0.1.2",
  "private": true,
  "packageManager": "pnpm@10.24.0",
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "typescript-plugin-scoped-imports": "workspace:*"
  },
  "scripts": {
    "build": "pnpm --filter typescript-plugin-scoped-imports run build",
    "typecheck": "pnpm --filter typescript-plugin-scoped-imports run typecheck",
    "watch": "pnpm --filter typescript-plugin-scoped-imports run watch",
    "test": "pnpm --filter typescript-plugin-scoped-imports run test",
    "biome:lint": "biome lint .",
    "biome:lint:fix": "biome lint --write .",
    "biome:format": "biome format .",
    "biome:format:fix": "biome format --write .",
    "biome:check": "biome check .",
    "biome:check:fix": "biome check --write ."
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  }
}
