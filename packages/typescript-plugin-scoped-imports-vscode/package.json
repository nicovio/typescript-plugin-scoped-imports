{
  "name": "typescript-plugin-scoped-imports-vscode",
  "displayName": "TypeScript Plugin Scoped Imports",
  "description": "Prevent out-of-scope auto-imports from __private__ folders in TypeScript projects.",
  "version": "0.1.9",
  "publisher": "nicovio",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nicovio/typescript-plugin-scoped-imports.git",
    "directory": "packages/typescript-plugin-scoped-imports-vscode"
  },
  "homepage": "https://github.com/nicovio/typescript-plugin-scoped-imports",
  "bugs": {
    "url": "https://github.com/nicovio/typescript-plugin-scoped-imports/issues"
  },
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": ["Programming Languages"],
  "keywords": ["typescript", "tsserver", "auto-import", "plugin"],
  "icon": "assets/icon.png",
  "files": [
    "README.md",
    "CHANGELOG.md",
    "LICENSE",
    "assets/icon.png",
    "assets/demo.gif",
    "node_modules/typescript-plugin-scoped-imports/**"
  ],
  "extensionKind": ["workspace"],
  "contributes": {
    "typescriptServerPlugins": [
      {
        "name": "typescript-plugin-scoped-imports",
        "enableForWorkspaceTypeScriptVersions": true
      }
    ]
  },
  "scripts": {
    "sync:plugin": "node ./scripts/sync-plugin.mjs",
    "package": "npm run sync:plugin && vsce package",
    "publish:vsce": "npm run sync:plugin && vsce publish"
  },
  "devDependencies": {
    "@vscode/vsce": "^3.6.1"
  }
}
