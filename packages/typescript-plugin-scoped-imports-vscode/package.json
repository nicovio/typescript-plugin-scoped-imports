{
  "name": "typescript-plugin-scoped-imports-vscode",
  "displayName": "TypeScript Plugin Scoped Imports",
  "description": "VS Code extension wrapper that loads typescript-plugin-scoped-imports in tsserver.",
  "version": "0.1.1",
  "publisher": "nicovio",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nicovio/typescript-plugin-scoped-imports.git",
    "directory": "packages/typescript-plugin-scoped-imports-vscode"
  },
  "homepage": "https://github.com/nicovio/typescript-plugin-scoped-imports",
  "bugs": {
    "url": "https://github.com/nicovio/typescript-plugin-scoped-imports/issues"
  },
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "typescript",
    "tsserver",
    "auto-import",
    "plugin"
  ],
  "files": [
    "README.md",
    "LICENSE",
    "plugin/**"
  ],
  "extensionKind": [
    "workspace"
  ],
  "contributes": {
    "typescriptServerPlugins": [
      {
        "name": "./plugin",
        "enableForWorkspaceTypeScriptVersions": true
      }
    ]
  },
  "scripts": {
    "sync:plugin": "node ./scripts/sync-plugin.mjs",
    "package": "npm run sync:plugin && vsce package --no-dependencies",
    "publish:vsce": "npm run sync:plugin && vsce publish --no-dependencies"
  },
  "devDependencies": {
    "@vscode/vsce": "^3.6.1"
  }
}
